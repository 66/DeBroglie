{
    "$schema": "../mxgmn/schema.json",
    "src": "./escheresque.csv",
    "dest": "output/escheresque.csv",
    //"dest": "output/escheresque.unity",
    "model": {
        "type": "adjacent",
    },
    "periodicInput": false,
    "periodic": false,
    "constraints": [
        {
            "type": "border",
            "tile": "1",
            "sides": "zmin"
        },
        {
            "type": "border",
            "tile": "0",
            "sides": "zmax"
        }
    ],
    "width": 100,
    "height": 100,
    "depth": 10,
    "backtrack": true
}